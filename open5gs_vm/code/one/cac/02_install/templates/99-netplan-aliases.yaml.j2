# Generated by ansible
network:
  version: 2
  renderer: networkd
  ethernets:
{% if one_open5gs_vm_internal_vnet|length == 0 %}
    eth0:
      addresses:
        - {{ ips[access_vnet_id] }}
        - {{ one_open5gs_vm_amf_ip }}/24
        - {{ one_open5gs_vm_upf_ip }}/24
{% elif one_open5gs_vm_internal_vnet|length == 1 %}
    eth1:
      addresses:
        - {{ ips[n2_vnet_id] }}
        - {{ one_open5gs_vm_amf_ip }}/24
        - {{ one_open5gs_vm_upf_ip }}/24
{% elif one_open5gs_vm_internal_vnet|length == 2 %}
    eth1:
      addresses:
        - {{ ips[n2_vnet_id] }}
        - {{ one_open5gs_vm_amf_ip }}/24
    eth2:
      addresses:
        - {{ ips[n3_vnet_id] }}
        - {{ one_open5gs_vm_upf_ip }}/24
{% endif %}