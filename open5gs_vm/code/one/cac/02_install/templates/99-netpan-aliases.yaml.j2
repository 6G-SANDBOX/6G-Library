# Generated by ansible
network:
  version: 2
  renderer: networkd
  ethernets:
{% if len(vm_ips) == 1 %}
    eth0:
      addresses:
        - {{ n2_ip }}
        - {{ n3_ip }}
{% elif len(vm_ips) == 2 %}
    eth1:
      addresses:
        - {{ n2_ip }}
        - {{ n3_ip }}
{% else %}
    eth1:
      addresses:
        - {{ n2_ip }}
    eth2:
      addresses:
        - {{ n3_ip }}
{% endif %}
