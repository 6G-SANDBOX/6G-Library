#!/bin/sh

MCC="{{ mcc }}"
MNC="{{ mnc }}"

open5gs-dbctl reset

{% for i in range(ue_count|int) %}
open5gs-dbctl add_ue_with_slice "{{ mcc }}{{ mnc }}{{ '%010d' % (ue_msin|int + i) }}" {{ ue_ki }} {{ ue_opc }} {{ ue_dnn }} 1 {{ slice_sd }}
{% endfor %}

open5gs-dbctl add_ue_with_slice ${MCC}${MNC}1100000001 00000000000000000000000000000000 66E94BD4EF8A2C3B884CFA59CA342B2E {{ ue_dnn }} 1 {{ slice_sd }}
open5gs-dbctl add_ue_with_slice ${MCC}${MNC}1100000002 00000000000000000000000000000000 66E94BD4EF8A2C3B884CFA59CA342B2E {{ ue_dnn }} 1 {{ slice_sd }}
