source ./ip_env

export MCC="{{ mcc | default('001') }}"
export MNC="{{ mnc | default('01') }}"

#export AMF_N2_IP=192.168.16.80
#export UPF1_N3_IP=192.168.17.23
#export UPF1_N3_IF=lo
export UPF1_N3_IP="{{ n3_ip | default('127.0.13.45') }}"
export UPF1_N3_IF="{{ n3_if | default('lo') }}"
export UPF1_N6_IP="{{ n6_ip | default('127.0.15.45') }}"
export UPF1_N6_IF="{{ n6_if | default('eth0') }}"
export AMF_N2_IP="{{ n2_ip | default('127.0.12.20') }}"

#export SQL_DB_IP=10.0.0.5

export PROMETHEUS_ENABLED=0     # 1|0        - enable prometheus metric server
#export COMMAND_DISABLED="false" # true|false - will hide the console prompt
#export DISABLE_REMOTE_COMMAND=0 # 0|1        - disables loading of remote-command module
#export UPF2_DISABLED=0          # 0|1        - disable upf2 in SMF
export DISABLE_XDP=1             # 0|1        - ignore XDP settings in UPF
export NRF_HB_ENABLE=0			 # 0|1		  - enable nrf heartbeats

